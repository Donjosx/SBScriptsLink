local player = game.Players.LocalPlayer
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = player:FindFirstChild("PlayerGui")

local frame = Instance.new("Frame")
frame.Size = UDim2.new(1, 0, 1, 0)
frame.BackgroundColor3 = Color3.fromRGB(0,0,0)
frame.Parent = screenGui

local message = Instance.new("TextLabel")
message.Size = UDim2.new(1, 0, 1, 0)
message.BackgroundColor3 = Color3.fromRGB(0,0,0)
message.TextColor3 = Color3.fromRGB(255,0,0)
message.TextStrokeTransparency = 0
message.Font = Enum.Font.SourceSansBold
message.TextScaled = true
message.Text = "IMPORTANT MESSAGE: MY YOUTUBE ACCOUNT GOT HACKED AND I AM TRYING TO GET IT BACK, PLEASE DON'T TRUST ANYTHING HE SAYS (INFORMATION NOT FROM SLAP BATTLE SCRIPTS) ALSO PLEASE SAY IT IN THE COMMENTS OF MY VIDEO FOR EVERYONE!!!"
message.Parent = frame
wait(15)
screenGui:Destroy()

loadstring(game:HttpGet(("https://raw.githubusercontent.com/Donjosx/SlapBattle_Scripts/main/Gloves-Abuser")))()
