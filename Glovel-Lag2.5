local function generateRandomName(length)
    local chars = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789"
    local randomName = ""
    for i = 1, length do
        local index = math.random(1, #chars)
        randomName = randomName .. chars:sub(index, index)
    end
    return randomName
end

